{
  "name": "symxify-client",
  "version": "1.0.1",
  "description": "A TypeScript client for the Symxify API",
  "main": "dist/client.js",
  "module": "dist/client.mjs",
  "types": "dist/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "gen:ops": "ts-node scripts/generate-operations-map.ts",
    "build": "tsup",
    "build:types": "tsc --emitDeclarationOnly --declaration --noEmitOnError false --outDir dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "tsup": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
